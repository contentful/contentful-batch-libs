{
  "name": "contentful-batch-libs",
  "version": "0.0.0-determined-by-semantic-release",
  "description": "Library modules used by contentful batch utility CLI tools.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/contentful-batch-libs.node.cjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./dist/contentful-batch-libs.node.cjs": "./dist/contentful-batch-libs.node.cjs",
    "./test/mocks/index.js": "./test/mocks/index.js"
  },
  "main": "./dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "react-native": "./dist/contentful-batch-libs.react-native.cjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "build:docs": "typedoc",
    "postbuild": "npm run check",
    "check": "npm run check:cjs && npm run check:esm",
    "check:cjs": "es-check es2021 './dist/**/*.cjs'",
    "check:esm": "es-check es2021 --module './dist/esm/*.js'",
    "clean": "rimraf dist && rimraf coverage",
    "lint": "eslint lib test/* test",
    "test": "npx vitest --run",
    "test:watch": "npx vitest",
    "test:cover": "npm run test -- --coverage",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@optimize-lodash/rollup-plugin": "^5.0.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@size-limit/file": "^11.2.0",
    "@types/json-patch": "^0.0.33",
    "@types/lodash": "^4.17.20",
    "@vitest/browser": "^3.2.4",
    "babel-loader": "^10.0.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "contentful-sdk-jsdoc": "^3.1.5",
    "date-fns": "^2.28.0",
    "es-check": "^9.2.0",
    "figures": "3.2.0",
    "https-proxy-agent": "^7.0.2",
    "in-publish": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.4",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "rollup": "^4.46.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "contentful-management": "^12.0.0-beta.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.1.7",
    "listr2": "^9.0.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^22.0.12",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful-batch-libs.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "contentful",
    "batch"
  ],
  "author": "Contentful <opensource@contentful.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contentful/contentful-batch-libs/issues"
  },
  "homepage": "https://github.com/contentful/contentful-batch-libs#readme",
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ]
  }
}
